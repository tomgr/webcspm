<html>
    <head>
        <title>CSPM Type Checker Results</title>
        <link rel="stylesheet" type="text/css" href="/static/screen.css"/>
    </head>
    <body>
        <div id="content">
            <h1>Type Check File</h1>

            <apply template="header"/>
            
            <div id="result">
                <h2>Result</h2>
                <result/>

                <h2>Source Code</h2>

                <div id="sourceCode">
                    <sourcecode/>
                </div>
            </div>

            <apply template="type_check_form"/>

            <apply template="footer"/>
        </div>
    </body>
</html>
